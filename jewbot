            local PreviewStroke = Create("UIStroke", {
                Color = Color3.fromRGB(100, 100, 100),
                Thickness = 1,
                Parent = ColorPreview
            })
            
            Colorpicker.GetValue = function() return value end
            Colorpicker.SetValue = function(newValue)
