-- Roblox UI Library
-- By: Your Name
-- Version: 1.0.0

local Library = {}
Library.__index = Library

-- Services
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local HttpService = game:GetService("HttpService")

-- Constants
local ACCENT_COLOR = Color3.fromRGB(146, 255, 155)
local BACKGROUND_COLOR = Color3.fromRGB(15, 15, 15)
local PANEL_COLOR = Color3.fromRGB(26, 26, 26)
local TEXT_COLOR = Color3.fromRGB(255, 255, 255)
local SUBTEXT_COLOR = Color3.fromRGB(200, 200, 200)

-- Theme System
Library.Theme = {
    BackgroundColor = BACKGROUND_COLOR,
    PanelColor = PANEL_COLOR,
    TextColor = TEXT_COLOR,
    SubTextColor = SUBTEXT_COLOR,
    AccentColor = ACCENT_COLOR,
    BorderRadius = 5,
    ShadowIntensity = 0.5,
    SliderFillColor = ACCENT_COLOR,
    ToggleColor = ACCENT_COLOR,
    ButtonColor = Color3.fromRGB(40, 40, 40),
    GraphLineColor = ACCENT_COLOR,
    GraphAxisColor = Color3.fromRGB(100, 100, 100)
}

-- Configuration System
Library.Configs = {}
Library.CurrentConfig = "default"
Library.AutoLoadConfig = nil
Library.AutoLoadTheme = nil

-- Utility Functions
local function Create(class, properties)
    local obj = Instance.new(class)
    for prop, value in pairs(properties) do
        obj[prop] = value
    end
    return obj
end

local function Tween(obj, properties, duration, easingStyle, easingDirection)
    local tweenInfo = TweenInfo.new(duration or 0.2, easingStyle or Enum.EasingStyle.Quad, easingDirection or Enum.EasingDirection.Out)
    local tween = TweenService:Create(obj, tweenInfo, properties)
    tween:Play()
    return tween
end

-- Main Library Functions
function Library.new()
    local self = setmetatable({}, Library)
    
    self.Elements = {}
    self.Windows = {}
    self.Notifications = {}
    self.Colorpickers = {}
    
    -- Create main screen GUI
    self.ScreenGui = Create("ScreenGui", {
        Name = "UILibrary",
        ResetOnSpawn = false,
        ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    })
    
    -- Load auto config/theme if specified
    if Library.AutoLoadConfig then
        self:LoadConfig(Library.AutoLoadConfig)
    end
    
    if Library.AutoLoadTheme then
        self:LoadTheme(Library.AutoLoadTheme)
    end
    
    return self
end

function Library:CreateWindow(title)
    local Window = {}
    
    -- Main container
    local MainFrame = Create("Frame", {
        Name = "MainWindow",
        BackgroundColor3 = Library.Theme.PanelColor,
        BorderColor3 = Color3.fromRGB(0, 0, 0),
        BorderSizePixel = 0,
        Position = UDim2.new(0.3, 0, 0.2, 0),
        Size = UDim2.new(0, 600, 0, 400),
        ClipsDescendants = true
    })
    
    local UICorner = Create("UICorner", {
        CornerRadius = UDim.new(0, Library.Theme.BorderRadius),
        Parent = MainFrame
    })
    
    local UIStroke = Create("UIStroke", {
        Color = Color3.fromRGB(40, 40, 40),
        Thickness = 1,
        Parent = MainFrame
    })
    
    local DropShadow = Create("ImageLabel", {
        Name = "DropShadow",
        BackgroundTransparency = 1,
        Position = UDim2.new(0, -15, 0, -15),
        Size = UDim2.new(1, 30, 1, 30),
        Image = "rbxassetid://5554236805",
        ImageColor3 = Color3.fromRGB(0, 0, 0),
        ImageTransparency = 0.8,
        ScaleType = Enum.ScaleType.Slice,
        SliceCenter = Rect.new(23, 23, 277, 277),
        Parent = MainFrame
    })
    
    -- Top bar
    local TopBar = Create("Frame", {
        Name = "TopBar",
        BackgroundColor3 = Color3.fromRGB(20, 20, 20),
        BorderSizePixel = 0,
        Size = UDim2.new(1, 0, 0, 30),
        Parent = MainFrame
    })
    
    local TitleLabel = Create("TextLabel", {
        Name = "Title",
        BackgroundTransparency = 1,
        Position = UDim2.new(0, 10, 0, 0),
        Size = UDim2.new(0, 200, 1, 0),
        Font = Enum.Font.Gotham,
        Text = title,
        TextColor3 = Library.Theme.TextColor,
        TextSize = 14,
        TextXAlignment = Enum.TextXAlignment.Left,
        Parent = TopBar
    })
    
    local VersionLabel = Create("TextLabel", {
        Name = "Version",
        BackgroundTransparency = 1,
        Position = UDim2.new(1, -60, 0, 0),
        Size = UDim2.new(0, 50, 1, 0),
        Font = Enum.Font.Gotham,
        Text = "v1.0",
        TextColor3 = Library.Theme.SubTextColor,
        TextSize = 12,
        Parent = TopBar
    })
    
    -- Search bar
    local SearchBox = Create("TextBox", {
        Name = "Search",
        BackgroundColor3 = Color3.fromRGB(30, 30, 30),
        BorderColor3 = Color3.fromRGB(60, 60, 60),
        BorderSizePixel = 1,
        Position = UDim2.new(0.5, -100, 0, 5),
        Size = UDim2.new(0, 200, 0, 20),
        Font = Enum.Font.Gotham,
        PlaceholderText = "Search...",
        PlaceholderColor3 = Color3.fromRGB(150, 150, 150),
        Text = "",
        TextColor3 = Library.Theme.TextColor,
        TextSize = 12,
        Parent = TopBar
    })
    
    local SearchUICorner = Create("UICorner", {
        CornerRadius = UDim.new(0, 4),
        Parent = SearchBox
    })
    
    -- Left icon bar
    local IconBar = Create("Frame", {
        Name = "IconBar",
        BackgroundColor3 = Color3.fromRGB(20, 20, 20),
        BorderSizePixel = 0,
        Position = UDim2.new(0, 0, 0, 30),
        Size = UDim2.new(0, 40, 1, -30),
        Parent = MainFrame
    })
    
    local IconLayout = Create("UIListLayout", {
        Padding = UDim.new(0, 5),
        HorizontalAlignment = Enum.HorizontalAlignment.Center,
        SortOrder = Enum.SortOrder.LayoutOrder,
        Parent = IconBar
    })
    
    -- Create 4 icons
    local icons = {"A", "B", "C", "D"}
    for i, icon in ipairs(icons) do
        local IconButton = Create("TextButton", {
            Name = "Icon"..i,
            BackgroundColor3 = Color3.fromRGB(30, 30, 30),
            BorderSizePixel = 0,
            Size = UDim2.new(0.7, 0, 0, 30),
            Font = Enum.Font.Gotham,
            Text = icon,
            TextColor3 = Library.Theme.TextColor,
            TextSize = 14,
            LayoutOrder = i,
            Parent = IconBar
        })
        
        local IconCorner = Create("UICorner", {
            CornerRadius = UDim.new(0, 4),
            Parent = IconButton
        })
        
        -- Last icon is settings
        if i == 4 then
            IconButton.Text = "âš™"
        end
    end
    
    -- Main content area
    local ContentArea = Create("Frame", {
        Name = "Content",
        BackgroundTransparency = 1,
        Position = UDim2.new(0, 40, 0, 30),
        Size = UDim2.new(1, -40, 1, -30),
        Parent = MainFrame
    })
    
    -- Two column layout
    local LeftColumn = Create("ScrollingFrame", {
        Name = "LeftColumn",
        BackgroundTransparency = 1,
        BorderSizePixel = 0,
        Size = UDim2.new(0.5, -5, 1, 0),
        CanvasSize = UDim2.new(0, 0, 0, 0),
        ScrollBarThickness = 3,
        ScrollBarImageColor3 = Color3.fromRGB(60, 60, 60),
        Parent = ContentArea
    })
    
    local RightColumn = Create("ScrollingFrame", {
        Name = "RightColumn",
        BackgroundTransparency = 1,
        BorderSizePixel = 0,
        Position = UDim2.new(0.5, 5, 0, 0),
        Size = UDim2.new(0.5, -5, 1, 0),
        CanvasSize = UDim2.new(0, 0, 0, 0),
        ScrollBarThickness = 3,
        ScrollBarImageColor3 = Color3.fromRGB(60, 60, 60),
        Parent = ContentArea
    })
    
    -- Store references
    Window.MainFrame = MainFrame
    Window.LeftColumn = LeftColumn
    Window.RightColumn = RightColumn
    Window.IconBar = IconBar
    Window.SearchBox = SearchBox
    Window.Elements = {}
    
    -- Make draggable
    local dragging = false
    local dragInput, dragStart, startPos
    
    TopBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = MainFrame.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    
    TopBar.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement then
            dragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if dragging and input == dragInput then
            local delta = input.Position - dragStart
            MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end)
    
    -- Add to screen
    MainFrame.Parent = self.ScreenGui
    table.insert(self.Windows, Window)
    
    -- Search functionality
    SearchBox:GetPropertyChangedSignal("Text"):Connect(function()
        local searchText = string.lower(SearchBox.Text)
        for _, element in pairs(Window.Elements) do
            if element:IsA("Frame") and element.Name:match("Section") then
                local visible = searchText == "" or string.find(string.lower(element.Title.Text), searchText)
                element.Visible = visible
            end
        end
    end)
    
    function Window:CreateSection(name, side)
        local column = side == "right" and RightColumn or LeftColumn
        local Section = {}
        
        local SectionFrame = Create("Frame", {
            Name = "Section_"..name,
            BackgroundColor3 = Color3.fromRGB(30, 30, 30),
            BorderSizePixel = 0,
            Size = UDim2.new(1, -10, 0, 200),
            LayoutOrder = #column:GetChildren(),
            Parent = column
        })
        
        local SectionCorner = Create("UICorner", {
            CornerRadius = UDim.new(0, Library.Theme.BorderRadius),
            Parent = SectionFrame
        })
        
        local SectionStroke = Create("UIStroke", {
            Color = Color3.fromRGB(50, 50, 50),
            Thickness = 1,
            Parent = SectionFrame
        })
        
        local SectionTitle = Create("TextLabel", {
            Name = "Title",
            BackgroundTransparency = 1,
            Position = UDim2.new(0, 10, 0, 5),
            Size = UDim2.new(1, -20, 0, 20),
            Font = Enum.Font.Gotham,
            Text = name,
            TextColor3 = Library.Theme.TextColor,
            TextSize = 16,
            TextXAlignment = Enum.TextXAlignment.Left,
            Parent = SectionFrame
        })
        
        local Content = Create("Frame", {
            Name = "Content",
            BackgroundTransparency = 1,
            Position = UDim2.new(0, 10, 0, 30),
            Size = UDim2.new(1, -20, 1, -35),
            Parent = SectionFrame
        })
        
        local ContentLayout = Create("UIListLayout", {
            Padding = UDim.new(0, 5),
            SortOrder = Enum.SortOrder.LayoutOrder,
            Parent = Content
        })
        
        ContentLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
            SectionFrame.Size = UDim2.new(1, -10, 0, ContentLayout.AbsoluteContentSize.Y + 40)
        end)
        
        Section.Frame = SectionFrame
        Section.Content = Content
        Section.Title = SectionTitle
        Section.Elements = {}
        
        table.insert(Window.Elements, SectionFrame)
        
        function Section:AddToggle(name, default, callback)
            local Toggle = {}
            local value = default or false
            
            local ToggleFrame = Create("Frame", {
                Name = "Toggle_"..name,
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 20),
                LayoutOrder = #Content:GetChildren(),
                Parent = Content
            })
            
            local ToggleLabel = Create("TextLabel", {
                Name = "Label",
                BackgroundTransparency = 1,
                Size = UDim2.new(0.7, 0, 1, 0),
                Font = Enum.Font.Gotham,
                Text = name,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 13,
                TextXAlignment = Enum.TextXAlignment.Left,
                Parent = ToggleFrame
            })
            
            local ToggleButton = Create("Frame", {
                Name = "Toggle",
                BackgroundColor3 = Color3.fromRGB(50, 50, 50),
                Position = UDim2.new(0.8, 0, 0, 2),
                Size = UDim2.new(0, 36, 0, 16),
                Parent = ToggleFrame
            })
            
            local ToggleCorner = Create("UICorner", {
                CornerRadius = UDim.new(1, 0),
                Parent = ToggleButton
            })
            
            local ToggleDot = Create("Frame", {
                Name = "Dot",
                BackgroundColor3 = Library.Theme.ToggleColor,
                Position = UDim2.new(0, 2, 0, 2),
                Size = UDim2.new(0, 12, 0, 12),
                Parent = ToggleButton
            })
            
            local DotCorner = Create("UICorner", {
                CornerRadius = UDim.new(1, 0),
                Parent = ToggleDot
            })
            
            local function UpdateToggle()
                if value then
                    Tween(ToggleDot, {Position = UDim2.new(1, -14, 0, 2)}, 0.2)
                    Tween(ToggleButton, {BackgroundColor3 = Library.Theme.ToggleColor:lerp(Color3.new(1,1,1), 0.3)}, 0.2)
                else
                    Tween(ToggleDot, {Position = UDim2.new(0, 2, 0, 2)}, 0.2)
                    Tween(ToggleButton, {BackgroundColor3 = Color3.fromRGB(50, 50, 50)}, 0.2)
                end
            end
            
            UpdateToggle()
            
            ToggleButton.InputBegan:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    value = not value
                    UpdateToggle()
                    if callback then callback(value) end
                end
            end)
            
            Toggle.GetValue = function() return value end
            Toggle.SetValue = function(newValue)
                value = newValue
                UpdateToggle()
                if callback then callback(value) end
            end
            
            table.insert(Section.Elements, Toggle)
            return Toggle
        end
        
        function Section:AddSlider(name, min, max, default, callback)
            local Slider = {}
            local value = default or min
            
            local SliderFrame = Create("Frame", {
                Name = "Slider_"..name,
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 40),
                LayoutOrder = #Content:GetChildren(),
                Parent = Content
            })
            
            local SliderLabel = Create("TextLabel", {
                Name = "Label",
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 20),
                Font = Enum.Font.Gotham,
                Text = name,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 13,
                TextXAlignment = Enum.TextXAlignment.Left,
                Parent = SliderFrame
            })
            
            local ValueLabel = Create("TextLabel", {
                Name = "Value",
                BackgroundTransparency = 1,
                Position = UDim2.new(1, -50, 0, 0),
                Size = UDim2.new(0, 50, 0, 20),
                Font = Enum.Font.Gotham,
                Text = tostring(value),
                TextColor3 = Library.Theme.SubTextColor,
                TextSize = 12,
                TextXAlignment = Enum.TextXAlignment.Right,
                Parent = SliderFrame
            })
            
            local SliderTrack = Create("Frame", {
                Name = "Track",
                BackgroundColor3 = Color3.fromRGB(50, 50, 50),
                Position = UDim2.new(0, 0, 0, 25),
                Size = UDim2.new(1, 0, 0, 6),
                Parent = SliderFrame
            })
            
            local TrackCorner = Create("UICorner", {
                CornerRadius = UDim.new(1, 0),
                Parent = SliderTrack
            })
            
            local SliderFill = Create("Frame", {
                Name = "Fill",
                BackgroundColor3 = Library.Theme.SliderFillColor,
                Size = UDim2.new(0, 0, 1, 0),
                Parent = SliderTrack
            })
            
            local FillCorner = Create("UICorner", {
                CornerRadius = UDim.new(1, 0),
                Parent = SliderFill
            })
            
            local SliderDot = Create("Frame", {
                Name = "Dot",
                BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                Position = UDim2.new(0, 0, 0.5, -8),
                Size = UDim2.new(0, 16, 0, 16),
                Parent = SliderTrack
            })
            
            local DotCorner = Create("UICorner", {
                CornerRadius = UDim.new(1, 0),
                Parent = SliderDot
            })
            
            local function UpdateSlider()
                local percent = (value - min) / (max - min)
                SliderFill.Size = UDim2.new(percent, 0, 1, 0)
                SliderDot.Position = UDim2.new(percent, -8, 0.5, -8)
                ValueLabel.Text = string.format("%.1f", value)
            end
            
            UpdateSlider()
            
            local dragging = false
            SliderTrack.InputBegan:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    dragging = true
                    local mousePos = UserInputService:GetMouseLocation()
                    local trackPos = SliderTrack.AbsolutePosition
                    local trackSize = SliderTrack.AbsoluteSize
                    
                    local percent = math.clamp((mousePos.X - trackPos.X) / trackSize.X, 0, 1)
                    value = min + (max - min) * percent
                    UpdateSlider()
                    if callback then callback(value) end
                end
            end)
            
            UserInputService.InputChanged:Connect(function(input)
                if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
                    local mousePos = input.Position
                    local trackPos = SliderTrack.AbsolutePosition
                    local trackSize = SliderTrack.AbsoluteSize
                    
                    local percent = math.clamp((mousePos.X - trackPos.X) / trackSize.X, 0, 1)
                    value = min + (max - min) * percent
                    UpdateSlider()
                    if callback then callback(value) end
                end
            end)
            
            UserInputService.InputEnded:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    dragging = false
                end
            end)
            
            Slider.GetValue = function() return value end
            Slider.SetValue = function(newValue)
                value = math.clamp(newValue, min, max)
                UpdateSlider()
                if callback then callback(value) end
            end
            
            table.insert(Section.Elements, Slider)
            return Slider
        end
        
        function Section:AddDropdown(name, options, default, callback)
            local Dropdown = {}
            local value = default or options[1]
            local open = false
            
            local DropdownFrame = Create("Frame", {
                Name = "Dropdown_"..name,
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 40),
                LayoutOrder = #Content:GetChildren(),
                Parent = Content
            })
            
            local DropdownLabel = Create("TextLabel", {
                Name = "Label",
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 20),
                Font = Enum.Font.Gotham,
                Text = name,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 13,
                TextXAlignment = Enum.TextXAlignment.Left,
                Parent = DropdownFrame
            })
            
            local DropdownButton = Create("TextButton", {
                Name = "Button",
                BackgroundColor3 = Color3.fromRGB(50, 50, 50),
                Position = UDim2.new(0, 0, 0, 25),
                Size = UDim2.new(1, 0, 0, 20),
                Font = Enum.Font.Gotham,
                Text = value,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 12,
                Parent = DropdownFrame
            })
            
            local ButtonCorner = Create("UICorner", {
                CornerRadius = UDim.new(0, 4),
                Parent = DropdownButton
            })
            
            local DropdownList = Create("ScrollingFrame", {
                Name = "List",
                BackgroundColor3 = Color3.fromRGB(40, 40, 40),
                BorderSizePixel = 0,
                Position = UDim2.new(0, 0, 0, 50),
                Size = UDim2.new(1, 0, 0, 0),
                CanvasSize = UDim2.new(0, 0, 0, 0),
                ScrollBarThickness = 3,
                Visible = false,
                Parent = DropdownFrame
            })
            
            local ListLayout = Create("UIListLayout", {
                SortOrder = Enum.SortOrder.LayoutOrder,
                Parent = DropdownList
            })
            
            for i, option in ipairs(options) do
                local OptionButton = Create("TextButton", {
                    Name = "Option_"..option,
                    BackgroundColor3 = Color3.fromRGB(50, 50, 50),
                    BorderSizePixel = 0,
                    Size = UDim2.new(1, 0, 0, 20),
                    Font = Enum.Font.Gotham,
                    Text = option,
                    TextColor3 = Library.Theme.TextColor,
                    TextSize = 12,
                    LayoutOrder = i,
                    Parent = DropdownList
                })
                
                OptionButton.MouseButton1Click:Connect(function()
                    value = option
                    DropdownButton.Text = value
                    Tween(DropdownList, {Size = UDim2.new(1, 0, 0, 0)}, 0.2)
                    wait(0.2)
                    DropdownList.Visible = false
                    open = false
                    if callback then callback(value) end
                end)
            end
            
            ListLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
                DropdownList.CanvasSize = UDim2.new(0, 0, 0, ListLayout.AbsoluteContentSize.Y)
            end)
            
            DropdownButton.MouseButton1Click:Connect(function()
                open = not open
                if open then
                    DropdownList.Visible = true
                    Tween(DropdownList, {Size = UDim2.new(1, 0, 0, math.min(ListLayout.AbsoluteContentSize.Y, 100))}, 0.2)
                else
                    Tween(DropdownList, {Size = UDim2.new(1, 0, 0, 0)}, 0.2)
                    wait(0.2)
                    DropdownList.Visible = false
                end
            end)
            
            Dropdown.GetValue = function() return value end
            Dropdown.SetValue = function(newValue)
                if table.find(options, newValue) then
                    value = newValue
                    DropdownButton.Text = value
                    if callback then callback(value) end
                end
            end
            
            table.insert(Section.Elements, Dropdown)
            return Dropdown
        end
        
        function Section:AddColorpicker(name, default, callback)
            local Colorpicker = {}
            local value = default or Library.Theme.AccentColor
            
            local ColorpickerFrame = Create("Frame", {
                Name = "Colorpicker_"..name,
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 30),
                LayoutOrder = #Content:GetChildren(),
                Parent = Content
            })
            
            local ColorpickerLabel = Create("TextLabel", {
                Name = "Label",
                BackgroundTransparency = 1,
                Size = UDim2.new(0.7, 0, 1, 0),
                Font = Enum.Font.Gotham,
                Text = name,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 13,
                TextXAlignment = Enum.TextXAlignment.Left,
                Parent = ColorpickerFrame
            })
            
            local ColorPreview = Create("Frame", {
                Name = "Preview",
                BackgroundColor3 = value,
                Position = UDim2.new(0.8, 0, 0, 5),
                Size = UDim2.new(0, 40, 0, 20),
                Parent = ColorpickerFrame
            })
            
            local PreviewCorner = Create("UICorner", {
                CornerRadius = UDim.new(0, 4),
                Parent = ColorPreview
            })
            
            local PreviewStroke = Create("UIStroke", {
                Color = Color3.fromRGB(100, 100, 100),
                Thickness = 1,
                Parent = ColorPreview
            })
            
            Colorpicker.GetValue = function() return value end
            Colorpicker.SetValue = function(newValue)
                value = newValue
                ColorPreview.BackgroundColor3 = value
                if callback then callback(value) end
            end
            
            table.insert(Section.Elements, Colorpicker)
            return Colorpicker
        end
        
        function Section:AddKeybind(name, default, callback)
            local Keybind = {}
            local value = default or Enum.KeyCode.LeftShift
            local listening = false
            
            local KeybindFrame = Create("Frame", {
                Name = "Keybind_"..name,
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 30),
                LayoutOrder = #Content:GetChildren(),
                Parent = Content
            })
            
            local KeybindLabel = Create("TextLabel", {
                Name = "Label",
                BackgroundTransparency = 1,
                Size = UDim2.new(0.7, 0, 1, 0),
                Font = Enum.Font.Gotham,
                Text = name,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 13,
                TextXAlignment = Enum.TextXAlignment.Left,
                Parent = KeybindFrame
            })
            
            local KeybindButton = Create("TextButton", {
                Name = "Button",
                BackgroundColor3 = Color3.fromRGB(50, 50, 50),
                Position = UDim2.new(0.8, 0, 0, 5),
                Size = UDim2.new(0, 80, 0, 20),
                Font = Enum.Font.Gotham,
                Text = tostring(value):gsub("Enum.KeyCode.", ""),
                TextColor3 = Library.Theme.TextColor,
                TextSize = 12,
                Parent = KeybindFrame
            })
            
            local ButtonCorner = Create("UICorner", {
                CornerRadius = UDim.new(0, 4),
                Parent = KeybindButton
            })
            
            KeybindButton.MouseButton1Click:Connect(function()
                listening = true
                KeybindButton.Text = "..."
                KeybindButton.BackgroundColor3 = Library.Theme.AccentColor
            end)
            
            local connection
            connection = UserInputService.InputBegan:Connect(function(input)
                if listening and input.UserInputType == Enum.UserInputType.Keyboard then
                    value = input.KeyCode
                    KeybindButton.Text = tostring(value):gsub("Enum.KeyCode.", "")
                    KeybindButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                    listening = false
                    connection:Disconnect()
                    if callback then callback(value) end
                end
            end)
            
            Keybind.GetValue = function() return value end
            Keybind.SetValue = function(newValue)
                value = newValue
                KeybindButton.Text = tostring(value):gsub("Enum.KeyCode.", "")
                if callback then callback(value) end
            end
            
            table.insert(Section.Elements, Keybind)
            return Keybind
        end
        
        function Section:AddTextbox(name, placeholder, callback)
            local Textbox = {}
            local value = ""
            
            local TextboxFrame = Create("Frame", {
                Name = "Textbox_"..name,
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 40),
                LayoutOrder = #Content:GetChildren(),
                Parent = Content
            })
            
            local TextboxLabel = Create("TextLabel", {
                Name = "Label",
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 20),
                Font = Enum.Font.Gotham,
                Text = name,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 13,
                TextXAlignment = Enum.TextXAlignment.Left,
                Parent = TextboxFrame
            })
            
            local TextboxInput = Create("TextBox", {
                Name = "Input",
                BackgroundColor3 = Color3.fromRGB(50, 50, 50),
                Position = UDim2.new(0, 0, 0, 25),
                Size = UDim2.new(1, 0, 0, 20),
                Font = Enum.Font.Gotham,
                PlaceholderText = placeholder,
                PlaceholderColor3 = Color3.fromRGB(150, 150, 150),
                Text = "",
                TextColor3 = Library.Theme.TextColor,
                TextSize = 12,
                Parent = TextboxFrame
            })
            
            local InputCorner = Create("UICorner", {
                CornerRadius = UDim.new(0, 4),
                Parent = TextboxInput
            })
            
            TextboxInput.FocusLost:Connect(function(enterPressed)
                value = TextboxInput.Text
                if callback then callback(value, enterPressed) end
            end)
            
            Textbox.GetValue = function() return value end
            Textbox.SetValue = function(newValue)
                value = newValue
                TextboxInput.Text = newValue
                if callback then callback(value) end
            end
            
            table.insert(Section.Elements, Textbox)
            return Textbox
        end
        
        function Section:AddButton(name, callback)
            local Button = {}
            
            local ButtonFrame = Create("Frame", {
                Name = "Button_"..name,
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 30),
                LayoutOrder = #Content:GetChildren(),
                Parent = Content
            })
            
            local ButtonElement = Create("TextButton", {
                Name = "Button",
                BackgroundColor3 = Library.Theme.ButtonColor,
                Size = UDim2.new(1, 0, 0, 25),
                Font = Enum.Font.Gotham,
                Text = name,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 13,
                Parent = ButtonFrame
            })
            
            local ButtonCorner = Create("UICorner", {
                CornerRadius = UDim.new(0, 4),
                Parent = ButtonElement
            })
            
            ButtonElement.MouseButton1Click:Connect(function()
                if callback then callback() end
            end)
            
            ButtonElement.MouseEnter:Connect(function()
                Tween(ButtonElement, {BackgroundColor3 = Library.Theme.ButtonColor:lerp(Color3.new(1,1,1), 0.1)}, 0.2)
            end)
            
            ButtonElement.MouseLeave:Connect(function()
                Tween(ButtonElement, {BackgroundColor3 = Library.Theme.ButtonColor}, 0.2)
            end)
            
            table.insert(Section.Elements, Button)
            return Button
        end
        
        function Section:AddLabel(text)
            local LabelFrame = Create("Frame", {
                Name = "Label_"..text,
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 20),
                LayoutOrder = #Content:GetChildren(),
                Parent = Content
            })
            
            local Label = Create("TextLabel", {
                Name = "Label",
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 1, 0),
                Font = Enum.Font.Gotham,
                Text = text,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 13,
                TextXAlignment = Enum.TextXAlignment.Left,
                Parent = LabelFrame
            })
            
            return Label
        end
        
        return Section
    end
        local MultiSection = {}
        local column = self.RightColumn
        
        local SectionFrame = Create("Frame", {
            Name = "MultiSection_"..name,
            BackgroundColor3 = Color3.fromRGB(30, 30, 30),
            BorderSizePixel = 0,
            Size = UDim2.new(1, -10, 0, 300),
            LayoutOrder = #column:GetChildren(),
            Parent = column
        })
        
        local SectionCorner = Create("UICorner", {
            CornerRadius = UDim.new(0, Library.Theme.BorderRadius),
            Parent = SectionFrame
        })
        
        local SectionStroke = Create("UIStroke", {
            Color = Color3.fromRGB(50, 50, 50),
            Thickness = 1,
            Parent = SectionFrame
        })
        
        local SectionTitle = Create("TextLabel", {
            Name = "Title",
            BackgroundTransparency = 1,
            Position = UDim2.new(0, 10, 0, 5),
            Size = UDim2.new(1, -20, 0, 20),
            Font = Enum.Font.Gotham,
            Text = name,
            TextColor3 = Library.Theme.TextColor,
            TextSize = 16,
            TextXAlignment = Enum.TextXAlignment.Left,
            Parent = SectionFrame
        })
        
        -- Tab bar
        local TabBar = Create("Frame", {
            Name = "TabBar",
            BackgroundTransparency = 1,
            Position = UDim2.new(0, 10, 0, 30),
            Size = UDim2.new(1, -20, 0, 30),
            Parent = SectionFrame
        })
        
        local TabLayout = Create("UIListLayout", {
            Padding = UDim.new(0, 5),
            FillDirection = Enum.FillDirection.Horizontal,
            SortOrder = Enum.SortOrder.LayoutOrder,
            Parent = TabBar
        })
        
        -- Create 6 tab buttons
        local tabs = {}
        for i = 1, 6 do
            local TabButton = Create("TextButton", {
                Name = "Tab"..i,
                BackgroundColor3 = Color3.fromRGB(50, 50, 50),
                BorderSizePixel = 0,
                Size = UDim2.new(0, 30, 0, 30),
                Font = Enum.Font.Gotham,
                Text = i,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 12,
                LayoutOrder = i,
                Parent = TabBar
            })
            
            local TabCorner = Create("UICorner", {
                CornerRadius = UDim.new(0.5, 0),
                Parent = TabButton
            })
            
            table.insert(tabs, TabButton)
        end
        
        local Content = Create("Frame", {
            Name = "Content",
            BackgroundTransparency = 1,
            Position = UDim2.new(0, 10, 0, 70),
            Size = UDim2.new(1, -20, 1, -75),
            Parent = SectionFrame
        })
        
        local ContentLayout = Create("UIListLayout", {
            Padding = UDim.new(0, 5),
            SortOrder = Enum.SortOrder.LayoutOrder,
            Parent = Content
        })
        
        ContentLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
            SectionFrame.Size = UDim2.new(1, -10, 0, math.max(ContentLayout.AbsoluteContentSize.Y + 80, 300))
        end)
        
        MultiSection.Frame = SectionFrame
        MultiSection.Content = Content
        MultiSection.Title = SectionTitle
        MultiSection.Tabs = tabs
        MultiSection.Elements = {}
        
        -- Multi-section elements (similar to Section)
        function MultiSection:AddSlider(name, min, max, default, callback)
            local Slider = {}
            local value = default or min
            
            local SliderFrame = Create("Frame", {
                Name = "Slider_"..name,
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 40),
                LayoutOrder = #Content:GetChildren(),
                Parent = Content
            })
            
            local SliderLabel = Create("TextLabel", {
                Name = "Label",
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 20),
                Font = Enum.Font.Gotham,
                Text = name,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 13,
                TextXAlignment = Enum.TextXAlignment.Left,
                Parent = SliderFrame
            })
            
            local ValueLabel = Create("TextLabel", {
                Name = "Value",
                BackgroundTransparency = 1,
                Position = UDim2.new(1, -50, 0, 0),
                Size = UDim2.new(0, 50, 0, 20),
                Font = Enum.Font.Gotham,
                Text = tostring(value),
                TextColor3 = Library.Theme.SubTextColor,
                TextSize = 12,
                TextXAlignment = Enum.TextXAlignment.Right,
                Parent = SliderFrame
            })
            
            local SliderTrack = Create("Frame", {
                Name = "Track",
                BackgroundColor3 = Color3.fromRGB(50, 50, 50),
                Position = UDim2.new(0, 0, 0, 25),
                Size = UDim2.new(1, 0, 0, 6),
                Parent = SliderFrame
            })
            
            local TrackCorner = Create("UICorner", {
                CornerRadius = UDim.new(1, 0),
                Parent = SliderTrack
            })
            
            local SliderFill = Create("Frame", {
                Name = "Fill",
                BackgroundColor3 = Library.Theme.SliderFillColor,
                Size = UDim2.new(0, 0, 1, 0),
                Parent = SliderTrack
            })
            
            local FillCorner = Create("UICorner", {
                CornerRadius = UDim.new(1, 0),
                Parent = SliderFill
            })
            
            local SliderDot = Create("Frame", {
                Name = "Dot",
                BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                Position = UDim2.new(0, 0, 0.5, -8),
                Size = UDim2.new(0, 16, 0, 16),
                Parent = SliderTrack
            })
            
            local DotCorner = Create("UICorner", {
                CornerRadius = UDim.new(1, 0),
                Parent = SliderDot
            })
            
            local function UpdateSlider()
                local percent = (value - min) / (max - min)
                SliderFill.Size = UDim2.new(percent, 0, 1, 0)
                SliderDot.Position = UDim2.new(percent, -8, 0.5, -8)
                ValueLabel.Text = string.format("%.1f", value)
            end
            
            UpdateSlider()
            
            local dragging = false
            SliderTrack.InputBegan:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    dragging = true
                    local mousePos = UserInputService:GetMouseLocation()
                    local trackPos = SliderTrack.AbsolutePosition
                    local trackSize = SliderTrack.AbsoluteSize
                    
                    local percent = math.clamp((mousePos.X - trackPos.X) / trackSize.X, 0, 1)
                    value = min + (max - min) * percent
                    UpdateSlider()
                    if callback then callback(value) end
                end
            end)
            
            UserInputService.InputChanged:Connect(function(input)
                if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
                    local mousePos = input.Position
                    local trackPos = SliderTrack.AbsolutePosition
                    local trackSize = SliderTrack.AbsoluteSize
                    
                    local percent = math.clamp((mousePos.X - trackPos.X) / trackSize.X, 0, 1)
                    value = min + (max - min) * percent
                    UpdateSlider()
                    if callback then callback(value) end
                end
            end)
            
            UserInputService.InputEnded:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    dragging = false
                end
            end)
            
            Slider.GetValue = function() return value end
            Slider.SetValue = function(newValue)
                value = math.clamp(newValue, min, max)
                UpdateSlider()
                if callback then callback(value) end
            end
            
            table.insert(MultiSection.Elements, Slider)
            return Slider
        end
        
        function MultiSection:AddKeybind(name, default, callback)
            local Keybind = {}
            local value = default or Enum.KeyCode.LeftShift
            local listening = false
            
            local KeybindFrame = Create("Frame", {
                Name = "Keybind_"..name,
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 30),
                LayoutOrder = #Content:GetChildren(),
                Parent = Content
            })
            
            local KeybindLabel = Create("TextLabel", {
                Name = "Label",
                BackgroundTransparency = 1,
                Size = UDim2.new(0.7, 0, 1, 0),
                Font = Enum.Font.Gotham,
                Text = name,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 13,
                TextXAlignment = Enum.TextXAlignment.Left,
                Parent = KeybindFrame
            })
            
            local KeybindButton = Create("TextButton", {
                Name = "Button",
                BackgroundColor3 = Color3.fromRGB(50, 50, 50),
                Position = UDim2.new(0.8, 0, 0, 5),
                Size = UDim2.new(0, 80, 0, 20),
                Font = Enum.Font.Gotham,
                Text = tostring(value):gsub("Enum.KeyCode.", ""),
                TextColor3 = Library.Theme.TextColor,
                TextSize = 12,
                Parent = KeybindFrame
            })
            
            local ButtonCorner = Create("UICorner", {
                CornerRadius = UDim.new(0, 4),
                Parent = KeybindButton
            })
            
            KeybindButton.MouseButton1Click:Connect(function()
                listening = true
                KeybindButton.Text = "..."
                KeybindButton.BackgroundColor3 = Library.Theme.AccentColor
            end)
            
            local connection
            connection = UserInputService.InputBegan:Connect(function(input)
                if listening and input.UserInputType == Enum.UserInputType.Keyboard then
                    value = input.KeyCode
                    KeybindButton.Text = tostring(value):gsub("Enum.KeyCode.", "")
                    KeybindButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                    listening = false
                    connection:Disconnect()
                    if callback then callback(value) end
                end
            end)
            
            Keybind.GetValue = function() return value end
            Keybind.SetValue = function(newValue)
                value = newValue
                KeybindButton.Text = tostring(value):gsub("Enum.KeyCode.", "")
                if callback then callback(value) end
            end
            
            table.insert(MultiSection.Elements, Keybind)
            return Keybind
        end
        
        function MultiSection:AddColorpicker(name, default, callback)
            local Colorpicker = {}
            local value = default or Library.Theme.AccentColor
            
            local ColorpickerFrame = Create("Frame", {
                Name = "Colorpicker_"..name,
                BackgroundTransparency = 1,
