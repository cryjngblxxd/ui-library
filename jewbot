        function MultiSection:AddDropdown(name, options, default, callback)
            local Dropdown = {}
            local value = default or options[1]
            local open = false
            
            local DropdownFrame = Create("Frame", {
                Name = "Dropdown_"..name,
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 40),
                LayoutOrder = #Content:GetChildren(),
                Parent = Content
            })
            
            local DropdownLabel = Create("TextLabel", {
                Name = "Label",
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 20),
                Font = Enum.Font.Gotham,
                Text = name,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 13,
                TextXAlignment = Enum.TextXAlignment.Left,
                Parent = DropdownFrame
            })
            
            local DropdownButton = Create("TextButton", {
                Name = "Button",
                BackgroundColor3 = Color3.fromRGB(50, 50, 50),
                Position = UDim2.new(0, 0, 0, 25),
                Size = UDim2.new(1, 0, 0, 20),
                Font = Enum.Font.Gotham,
                Text = value,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 12,
                Parent = DropdownFrame
            })
            
            local ButtonCorner = Create("UICorner", {
                CornerRadius = UDim.new(0, 4),
                Parent = DropdownButton
            })
            
            local DropdownList = Create("ScrollingFrame", {
                Name = "List",
                BackgroundColor3 = Color3.fromRGB(40, 40, 40),
                BorderSizePixel = 0,
                Position = UDim2.new(0, 0, 0, 50),
                Size = UDim2.new(1, 0, 0, 0),
                CanvasSize = UDim2.new(0, 0, 0, 0),
                ScrollBarThickness = 3,
                Visible = false,
                Parent = DropdownFrame
            })
            
            local ListLayout = Create("UIListLayout", {
                SortOrder = Enum.SortOrder.LayoutOrder,
                Parent = DropdownList
            })
            
            for i, option in ipairs(options) do
                local OptionButton = Create("TextButton", {
                    Name = "Option_"..option,
                    BackgroundColor3 = Color3.fromRGB(50, 50, 50),
                    BorderSizePixel = 0,
                    Size = UDim2.new(1, 0, 0, 20),
                    Font = Enum.Font.Gotham,
                    Text = option,
                    TextColor3 = Library.Theme.TextColor,
                    TextSize = 12,
                    LayoutOrder = i,
                    Parent = DropdownList
                })
                
                OptionButton.MouseButton1Click:Connect(function()
                    value = option
                    DropdownButton.Text = value
                    Tween(DropdownList, {Size = UDim2.new(1, 0, 0, 0)}, 0.2)
                    wait(0.2)
                    DropdownList.Visible = false
                    open = false
                    if callback then callback(value) end
                end)
            end
            
            ListLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
                DropdownList.CanvasSize = UDim2.new(0, 0, 0, ListLayout.AbsoluteContentSize.Y)
            end)
            
            DropdownButton.MouseButton1Click:Connect(function()
                open = not open
                if open then
                    DropdownList.Visible = true
                    Tween(DropdownList, {Size = UDim2.new(1, 0, 0, math.min(ListLayout.AbsoluteContentSize.Y, 100))}, 0.2)
                else
                    Tween(DropdownList, {Size = UDim2.new(1, 0, 0, 0)}, 0.2)
                    wait(0.2)
                    DropdownList.Visible = false
                end
            end)
            
            Dropdown.GetValue = function() return value end
            Dropdown.SetValue = function(newValue)
                if table.find(options, newValue) then
                    value = newValue
                    DropdownButton.Text = value
                    if callback then callback(value) end
                end
            end
            
            table.insert(MultiSection.Elements, Dropdown)
            return Dropdown
        end
        
        function MultiSection:AddGraph(graphType, callback)
            local Graph = {}
            
            local GraphFrame = Create("Frame", {
                Name = "Graph",
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 100),
                LayoutOrder = #Content:GetChildren(),
                Parent = Content
            })
            
            local GraphCanvas = Create("Frame", {
                Name = "Canvas",
                BackgroundColor3 = Color3.fromRGB(40, 40, 40),
                BorderSizePixel = 0,
                Size = UDim2.new(1, 0, 0, 80),
                Parent = GraphFrame
            })
            
            local CanvasCorner = Create("UICorner", {
                CornerRadius = UDim.new(0, 4),
                Parent = GraphCanvas
            })
            
            local CanvasStroke = Create("UIStroke", {
                Color = Color3.fromRGB(60, 60, 60),
                Thickness = 1,
                Parent = GraphCanvas
            })
            
            -- Draw grid
            for i = 1, 10 do
                local Line = Create("Frame", {
                    Name = "GridLine"..i,
                    BackgroundColor3 = Color3.fromRGB(50, 50, 50),
                    BorderSizePixel = 0,
                    Position = UDim2.new((i-1)/10, 0, 0, 0),
                    Size = UDim2.new(0, 1, 1, 0),
                    Parent = GraphCanvas
                })
            end
            
            -- Draw sine wave
            local points = {}
            for x = 0, 100 do
                local y = 40 + 30 * math.sin((x / 100) * 2 * math.pi)
                table.insert(points, {x, y})
            end
            
            for i = 1, #points - 1 do
                local p1 = points[i]
                local p2 = points[i + 1]
                
                local Line = Create("Frame", {
                    Name = "Line"..i,
                    BackgroundColor3 = Library.Theme.GraphLineColor,
                    BorderSizePixel = 0,
                    Position = UDim2.new((p1[1]/100), 0, 0, p1[2]),
                    Size = UDim2.new((p2[1]-p1[1])/100, 0, 0, 1),
                    Parent = GraphCanvas
                })
            end
            
            local InfoLabel = Create("TextLabel", {
                Name = "Info",
                BackgroundTransparency = 1,
                Position = UDim2.new(0, 0, 0, 85),
                Size = UDim2.new(1, 0, 0, 20),
                Font = Enum.Font.Gotham,
                Text = "X: 0 | Y: 0",
                TextColor3 = Library.Theme.SubTextColor,
                TextSize = 12,
                TextXAlignment = Enum.TextXAlignment.Center,
                Parent = GraphFrame
            })
            
            table.insert(MultiSection.Elements, Graph)
            return Graph
        end
        
        function MultiSection:AddButton(name, callback)
            local Button = {}
            
            local ButtonFrame = Create("Frame", {
                Name = "Button_"..name,
                BackgroundTransparency = 1,
                Size = UDim2.new(0.5, -3, 0, 30),
                LayoutOrder = #Content:GetChildren(),
                Parent = Content
            })
            
            local ButtonElement = Create("TextButton", {
                Name = "Button",
                BackgroundColor3 = Library.Theme.ButtonColor,
                Size = UDim2.new(1, 0, 0, 25),
                Font = Enum.Font.Gotham,
                Text = name,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 13,
                Parent = ButtonElement
            })
            
            local ButtonCorner = Create("UICorner", {
                CornerRadius = UDim.new(0, 4),
                Parent = ButtonElement
            })
            
            ButtonElement.MouseButton1Click:Connect(function()
                if callback then callback() end
            end)
            
            ButtonElement.MouseEnter:Connect(function()
                Tween(ButtonElement, {BackgroundColor3 = Library.Theme.ButtonColor:lerp(Color3.new(1,1,1), 0.1)}, 0.2)
            end)
            
            ButtonElement.MouseLeave:Connect(function()
                Tween(ButtonElement, {BackgroundColor3 = Library.Theme.ButtonColor}, 0.2)
            end)
            
            table.insert(MultiSection.Elements, Button)
            return Button
        end
        
        table.insert(Window.Elements, SectionFrame)
        return MultiSection
    end

-- Colorpicker Panel (Floating)
function Library:CreateColorpickerPanel()
    local ColorpickerPanel = {}
    
    local PanelFrame = Create("Frame", {
        Name = "ColorpickerPanel",
        BackgroundColor3 = Library.Theme.PanelColor,
        BorderSizePixel = 0,
        Position = UDim2.new(0, 10, 0, 10),
        Size = UDim2.new(0, 250, 0, 350),
        Parent = self.ScreenGui
    })
    
    local PanelCorner = Create("UICorner", {
        CornerRadius = UDim.new(0, Library.Theme.BorderRadius),
        Parent = PanelFrame
    })
    
    local PanelStroke = Create("UIStroke", {
        Color = Color3.fromRGB(50, 50, 50),
        Thickness = 1,
        Parent = PanelFrame
    })
    
    -- Title bar
    local TitleBar = Create("Frame", {
        Name = "TitleBar",
        BackgroundColor3 = Color3.fromRGB(20, 20, 20),
        BorderSizePixel = 0,
        Size = UDim2.new(1, 0, 0, 30),
        Parent = PanelFrame
    })
    
    local TitleLabel = Create("TextLabel", {
        Name = "Title",
        BackgroundTransparency = 1,
        Position = UDim2.new(0, 10, 0, 0),
        Size = UDim2.new(1, -20, 1, 0),
        Font = Enum.Font.Gotham,
        Text = "Colorpicker",
        TextColor3 = Library.Theme.TextColor,
        TextSize = 14,
        TextXAlignment = Enum.TextXAlignment.Left,
        Parent = TitleBar
    })
    
    -- Color gradient selector
    local GradientBox = Create("Frame", {
        Name = "GradientBox",
        BackgroundColor3 = Library.Theme.AccentColor,
        BorderSizePixel = 0,
        Position = UDim2.new(0, 10, 0, 35),
        Size = UDim2.new(0, 190, 0, 190),
        Parent = PanelFrame
    })
    
    local GradientCorner = Create("UICorner", {
        CornerRadius = UDim.new(0, 4),
        Parent = GradientBox
    })
    
    local GradientStroke = Create("UIStroke", {
        Color = Color3.fromRGB(100, 100, 100),
        Thickness = 1,
        Parent = GradientBox
    })
    
    -- Hue slider
    local HueSlider = Create("Frame", {
        Name = "HueSlider",
        BackgroundColor3 = Color3.fromRGB(50, 50, 50),
        BorderSizePixel = 0,
        Position = UDim2.new(0, 205, 0, 35),
        Size = UDim2.new(0, 30, 0, 190),
        Parent = PanelFrame
    })
    
    local HueCorner = Create("UICorner", {
        CornerRadius = UDim.new(1, 0),
        Parent = HueSlider
    })
    
    -- Brightness/Alpha bar
    local BrightnessBar = Create("Frame", {
        Name = "BrightnessBar",
        BackgroundColor3 = Color3.fromRGB(50, 50, 50),
        BorderSizePixel = 0,
        Position = UDim2.new(0, 10, 0, 230),
        Size = UDim2.new(0, 225, 0, 10),
        Parent = PanelFrame
    })
    
    local BrightnessCorner = Create("UICorner", {
        CornerRadius = UDim.new(1, 0),
        Parent = BrightnessBar
    })
    
    -- Toggles
    local SyncToggle = Create("Frame", {
        Name = "SyncToggle",
        BackgroundTransparency = 1,
        Position = UDim2.new(0, 10, 0, 245),
        Size = UDim2.new(1, -20, 0, 20),
        Parent = PanelFrame
    })
    
    local SyncLabel = Create("TextLabel", {
        Name = "Label",
        BackgroundTransparency = 1,
        Size = UDim2.new(0.6, 0, 1, 0),
        Font = Enum.Font.Gotham,
        Text = "Sync colorpickers",
        TextColor3 = Library.Theme.TextColor,
        TextSize = 12,
        TextXAlignment = Enum.TextXAlignment.Left,
        Parent = SyncToggle
    })
    
    local SyncButton = Create("Frame", {
        Name = "Toggle",
        BackgroundColor3 = Color3.fromRGB(50, 50, 50),
        Position = UDim2.new(0.8, 0, 0, 2),
        Size = UDim2.new(0, 36, 0, 16),
        Parent = SyncToggle
    })
    
    local SyncButtonCorner = Create("UICorner", {
        CornerRadius = UDim.new(1, 0),
        Parent = SyncButton
    })
    
    local SyncDot = Create("Frame", {
        Name = "Dot",
        BackgroundColor3 = Library.Theme.ToggleColor,
        Position = UDim2.new(0, 2, 0, 2),
        Size = UDim2.new(0, 12, 0, 12),
        Parent = SyncButton
    })
    
    local SyncDotCorner = Create("UICorner", {
        CornerRadius = UDim.new(1, 0),
        Parent = SyncDot
    })
    
    -- Rainbow toggle
    local RainbowToggle = Create("Frame", {
        Name = "RainbowToggle",
        BackgroundTransparency = 1,
        Position = UDim2.new(0, 10, 0, 270),
        Size = UDim2.new(1, -20, 0, 20),
        Parent = PanelFrame
    })
    
    local RainbowLabel = Create("TextLabel", {
        Name = "Label",
        BackgroundTransparency = 1,
        Size = UDim2.new(0.6, 0, 1, 0),
        Font = Enum.Font.Gotham,
        Text = "Rainbow",
        TextColor3 = Library.Theme.TextColor,
        TextSize = 12,
        TextXAlignment = Enum.TextXAlignment.Left,
        Parent = RainbowToggle
    })
    
    local RainbowButton = Create("Frame", {
        Name = "Toggle",
        BackgroundColor3 = Color3.fromRGB(50, 50, 50),
        Position = UDim2.new(0.8, 0, 0, 2),
        Size = UDim2.new(0, 36, 0, 16),
        Parent = RainbowToggle
    })
    
    local RainbowButtonCorner = Create("UICorner", {
        CornerRadius = UDim.new(1, 0),
        Parent = RainbowButton
    })
    
    local RainbowDot = Create("Frame", {
        Name = "Dot",
        BackgroundColor3 = Library.Theme.ToggleColor,
        Position = UDim2.new(0, 2, 0, 2),
        Size = UDim2.new(0, 12, 0, 12),
        Parent = RainbowButton
    })
    
    local RainbowDotCorner = Create("UICorner", {
        CornerRadius = UDim.new(1, 0),
        Parent = RainbowDot
    })
    
    -- Hex input
    local HexFrame = Create("Frame", {
        Name = "HexFrame",
        BackgroundTransparency = 1,
        Position = UDim2.new(0, 10, 0, 295),
        Size = UDim2.new(1, -20, 0, 25),
        Parent = PanelFrame
    })
    
    local HexLabel = Create("TextLabel", {
        Name = "Label",
        BackgroundTransparency = 1,
        Size = UDim2.new(0, 40, 1, 0),
        Font = Enum.Font.Gotham,
        Text = "Hex:",
        TextColor3 = Library.Theme.TextColor,
        TextSize = 12,
        TextXAlignment = Enum.TextXAlignment.Left,
        Parent = HexFrame
    })
    
    local HexInput = Create("TextBox", {
        Name = "Input",
        BackgroundColor3 = Color3.fromRGB(50, 50, 50),
        Position = UDim2.new(0, 50, 0, 0),
        Size = UDim2.new(1, -50, 0, 20),
        Font = Enum.Font.Gotham,
        PlaceholderText = "#92ff9b",
        PlaceholderColor3 = Color3.fromRGB(150, 150, 150),
        Text = "",
        TextColor3 = Library.Theme.TextColor,
        TextSize = 12,
        Parent = HexFrame
    })
    
    local HexInputCorner = Create("UICorner", {
        CornerRadius = UDim.new(0, 4),
        Parent = HexInput
    })
    
    -- RGBA input
    local RGBAFrame = Create("Frame", {
        Name = "RGBAFrame",
        BackgroundTransparency = 1,
        Position = UDim2.new(0, 10, 0, 325),
        Size = UDim2.new(1, -20, 0, 25),
        Parent = PanelFrame
    })
    
    local RGBALabel = Create("TextLabel", {
        Name = "Label",
        BackgroundTransparency = 1,
        Size = UDim2.new(0, 40, 1, 0),
        Font = Enum.Font.Gotham,
        Text = "RGBA:",
        TextColor3 = Library.Theme.TextColor,
        TextSize = 12,
        TextXAlignment = Enum.TextXAlignment.Left,
        Parent = RGBAFrame
    })
    
    local RGBAInput = Create("TextBox", {
        Name = "Input",
        BackgroundColor3 = Color3.fromRGB(50, 50, 50),
        Position = UDim2.new(0, 50, 0, 0),
        Size = UDim2.new(1, -50, 0, 20),
        Font = Enum.Font.Gotham,
        PlaceholderText = "146, 255, 155",
        PlaceholderColor3 = Color3.fromRGB(150, 150, 150),
        Text = "",
        TextColor3 = Library.Theme.TextColor,
        TextSize = 12,
        Parent = RGBAFrame
    })
    
    local RGBAInputCorner = Create("UICorner", {
        CornerRadius = UDim.new(0, 4),
        Parent = RGBAInput
    })
    
    -- Make draggable
    local dragging = false
    local dragInput, dragStart, startPos
    
    TitleBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = PanelFrame.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    
    TitleBar.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement then
            dragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if dragging and input == dragInput then
            local delta = input.Position - dragStart
            PanelFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end)
    
    ColorpickerPanel.Frame = PanelFrame
    return ColorpickerPanel
end

-- Tooltip
function Library:CreateTooltip(parent, text)
    local Tooltip = Create("Frame", {
        Name = "Tooltip",
        BackgroundColor3 = Color3.fromRGB(50, 50, 50),
        BorderSizePixel = 0,
        Visible = false,
        Parent = self.ScreenGui
    })
    
    local TooltipCorner = Create("UICorner", {
        CornerRadius = UDim.new(0, 4),
        Parent = Tooltip
    })
    
    local TooltipText = Create("TextLabel", {
        Name = "Text",
        BackgroundTransparency = 1,
        Position = UDim2.new(0, 5, 0, 5),
        Size = UDim2.new(1, -10, 1, -10),
        Font = Enum.Font.Gotham,
        Text = text,
        TextColor3 = Library.Theme.TextColor,
        TextSize = 11,
        TextWrapped = true,
        Parent = Tooltip
    })
    
    parent.MouseEnter:Connect(function()
        Tooltip.Visible = true
        local parentPos = parent.AbsolutePosition
        Tooltip.Position = UDim2.new(0, parentPos.X + 10, 0, parentPos.Y - 30)
        Tooltip.Size = UDim2.new(0, 150, 0, 25)
    end)
    
    parent.MouseLeave:Connect(function()
        Tooltip.Visible = false
    end)
    
    return Tooltip
end

-- Notification System
function Library:CreateNotification(title, description, duration, notificationType)
    local Notification = {}
    local notificationType = notificationType or "normal"
    
    local NotificationFrame = Create("Frame", {
        Name = "Notification",
        BackgroundColor3 = Library.Theme.PanelColor,
        BorderSizePixel = 0,
        Position = UDim2.new(1, -320, 0, 10 + (#self.Notifications * 110)),
        Size = UDim2.new(0, 300, 0, 100),
        Parent = self.ScreenGui
    })
    
    local NotificationCorner = Create("UICorner", {
        CornerRadius = UDim.new(0, Library.Theme.BorderRadius),
        Parent = NotificationFrame
    })
    
    local NotificationStroke = Create("UIStroke", {
        Color = Color3.fromRGB(50, 50, 50),
        Thickness = 1,
        Parent = NotificationFrame
    })
    
    -- Title
    local TitleLabel = Create("TextLabel", {
        Name = "Title",
        BackgroundTransparency = 1,
        Position = UDim2.new(0, 10, 0, 5),
        Size = UDim2.new(1, -20, 0, 20),
        Font = Enum.Font.Gotham,
        Text = title,
        TextColor3 = Library.Theme.TextColor,
        TextSize = 14,
        TextXAlignment = Enum.TextXAlignment.Left,
        Parent = NotificationFrame
    })
    
    -- Description
    local DescriptionLabel = Create("TextLabel", {
        Name = "Description",
        BackgroundTransparency = 1,
        Position = UDim2.new(0, 10, 0, 25),
        Size = UDim2.new(1, -20, 0, 50),
        Font = Enum.Font.Gotham,
        Text = description,
        TextColor3 = Library.Theme.SubTextColor,
        TextSize = 12,
        TextWrapped = true,
        TextXAlignment = Enum.TextXAlignment.Left,
        TextYAlignment = Enum.TextYAlignment.Top,
        Parent = NotificationFrame
    })
    
    if notificationType == "yesno" then
        -- Yes button
        local YesButton = Create("TextButton", {
            Name = "Yes",
            BackgroundColor3 = Color3.fromRGB(0, 255, 100),
            Position = UDim2.new(0, 10, 1, -30),
            Size = UDim2.new(0.45, -5, 0, 25),
            Font = Enum.Font.Gotham,
            Text = "Yes",
            TextColor3 = Color3.fromRGB(0, 0, 0),
            TextSize = 12,
            Parent = NotificationFrame
        })
        
        local YesCorner = Create("UICorner", {
            CornerRadius = UDim.new(0, 4),
            Parent = YesButton
        })
        
        -- No button
        local NoButton = Create("TextButton", {
            Name = "No",
            BackgroundColor3 = Color3.fromRGB(255, 50, 50),
            Position = UDim2.new(0.55, 5, 1, -30),
            Size = UDim2.new(0.45, -5, 0, 25),
            Font = Enum.Font.Gotham,
            Text = "No",
            TextColor3 = Color3.fromRGB(255, 255, 255),
            TextSize = 12,
            Parent = NotificationFrame
        })
        
        local NoCorner = Create("UICorner", {
            CornerRadius = UDim.new(0, 4),
            Parent = NoButton
        })
        
        NotificationFrame.Size = UDim2.new(0, 300, 0, 120)
        
    elseif notificationType == "infinite" then
        -- Dismiss button
        local DismissButton = Create("TextButton", {
            Name = "Dismiss",
            BackgroundColor3 = Library.Theme.ButtonColor,
            Position = UDim2.new(0, 10, 1, -30),
            Size = UDim2.new(1, -20, 0, 25),
            Font = Enum.Font.Gotham,
            Text = "Dismiss",
            TextColor3 = Library.Theme.TextColor,
            TextSize = 12,
            Parent = NotificationFrame
        })
        
        local DismissCorner = Create("UICorner", {
            CornerRadius = UDim.new(0, 4),
            Parent = DismissButton
        })
        
        DismissButton.MouseButton1Click:Connect(function()
            Tween(NotificationFrame, {Position = UDim2.new(1, 0, 0, NotificationFrame.Position.Y.Offset)}, 0.3)
            wait(0.3)
            NotificationFrame:Destroy()
            table.remove(self.Notifications, table.find(self.Notifications, Notification))
        end)
        
        NotificationFrame.Size = UDim2.new(0, 300, 0, 120)
    else
        -- Timed notification - auto dismiss
        if duration then
            task.wait(duration)
            Tween(NotificationFrame, {Position = UDim2.new(1, 0, 0, NotificationFrame.Position.Y.Offset)}, 0.3)
            wait(0.3)
            NotificationFrame:Destroy()
            table.remove(self.Notifications, table.find(self.Notifications, Notification))
        end
    end
    
    table.insert(self.Notifications, Notification)
    return Notification
end

-- Configuration System
function Library:SaveConfig(name)
    local config = {}
    
    for _, window in pairs(self.Windows) do
        for _, element in pairs(window.Elements) do
            if element:IsA("Frame") and element.Name:match("Section") then
                local sectionData = {}
                
                for _, child in pairs(element:FindFirstChild("Content"):GetChildren()) do
                    if child:IsA("Frame") and child.Name:match("Toggle") then
                        local toggle = child:FindFirstChild("Toggle")
                        if toggle then
                            sectionData[child.Name] = toggle:FindFirstChild("Dot").Position
                        end
                    elseif child:IsA("Frame") and child.Name:match("Slider") then
                        local slider = child:FindFirstChild("Track")
                        if slider then
                            sectionData[child.Name] = slider:FindFirstChild("Fill").Size
                        end
                    elseif child:IsA("Frame") and child.Name:match("Textbox") then
                        local textbox = child:FindFirstChild("Input")
                        if textbox then
                            sectionData[child.Name] = textbox.Text
                        end
                    end
                end
                
                config[element.Name] = sectionData
            end
        end
    end
    
    Library.Configs[name] = config
    
    -- Save to JSON file simulation
    local jsonData = HttpService:JSONEncode(config)
    print("Config saved: "..name)
    
    return config
end

function Library:LoadConfig(name)
    if not Library.Configs[name] then
        print("Config not found: "..name)
        return
    end
    
    local config = Library.Configs[name]
    Library.CurrentConfig = name
    print("Config loaded: "..name)
end

function Library:DeleteConfig(name)
    Library.Configs[name] = nil
    if Library.CurrentConfig == name then
        Library.CurrentConfig = "default"
    end
    print("Config deleted: "..name)
end

-- Theme System
function Library:SaveTheme(name, themeData)
    Library.Themes = Library.Themes or {}
    Library.Themes[name] = themeData or {
        BackgroundColor = Library.Theme.BackgroundColor,
        PanelColor = Library.Theme.PanelColor,
        TextColor = Library.Theme.TextColor,
        SubTextColor = Library.Theme.SubTextColor,
        AccentColor = Library.Theme.AccentColor,
        BorderRadius = Library.Theme.BorderRadius,
        ShadowIntensity = Library.Theme.ShadowIntensity,
        SliderFillColor = Library.Theme.SliderFillColor,
        ToggleColor = Library.Theme.ToggleColor,
        ButtonColor = Library.Theme.ButtonColor,
        GraphLineColor = Library.Theme.GraphLineColor,
        GraphAxisColor = Library.Theme.GraphAxisColor
    }
    
    print("Theme saved: "..name)
end

function Library:LoadTheme(name)
    Library.Themes = Library.Themes or {}
    if not Library.Themes[name] then
        print("Theme not found: "..name)
        return
    end
    
    local themeData = Library.Themes[name]
    for key, value in pairs(themeData) do
        Library.Theme[key] = value
    end
    
    print("Theme loaded: "..name)
end

function Library:ApplyTheme(themeData)
    for key, value in pairs(themeData) do
        Library.Theme[key] = value
    end
    print("Theme applied")
end

-- Attach to ScreenGui
function Library:Attach(parent)
    parent = parent or game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")
    self.ScreenGui.Parent = parent
    return self
end

-- Destroy Library
function Library:Destroy()
    self.ScreenGui:Destroy()
end

return LoadLibrary
