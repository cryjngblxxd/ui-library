                ColorPreview.BackgroundColor3 = value
                if callback then callback(value) end
            end
            
            table.insert(MultiSection.Elements, Colorpicker)
            return Colorpicker
        end
        
        function MultiSection:AddToggle(name, default, callback)
            local Toggle = {}
            local value = default or false
            
            local ToggleFrame = Create("Frame", {
                Name = "Toggle_"..name,
                BackgroundTransparency = 1,
                Size = UDim2.new(1, 0, 0, 20),
                LayoutOrder = #Content:GetChildren(),
                Parent = Content
            })
            
            local ToggleLabel = Create("TextLabel", {
                Name = "Label",
                BackgroundTransparency = 1,
                Size = UDim2.new(0.7, 0, 1, 0),
                Font = Enum.Font.Gotham,
                Text = name,
                TextColor3 = Library.Theme.TextColor,
                TextSize = 13,
                TextXAlignment = Enum.TextXAlignment.Left,
                Parent = ToggleFrame
            })
            
            local ToggleButton = Create("Frame", {
                Name = "Toggle",
                BackgroundColor3 = Color3.fromRGB(50, 50, 50),
                Position = UDim2.new(0.8, 0, 0, 2),
                Size = UDim2.new(0, 36, 0, 16),
                Parent = ToggleFrame
            })
            
            local ToggleCorner = Create("UICorner", {
                CornerRadius = UDim.new(1, 0),
                Parent = ToggleButton
            })
            
            local ToggleDot = Create("Frame", {
                Name = "Dot",
                BackgroundColor3 = Library.Theme.ToggleColor,
                Position = UDim2.new(0, 2, 0, 2),
                Size = UDim2.new(0, 12, 0, 12),
                Parent = ToggleButton
            })
            
            local DotCorner = Create("UICorner", {
                CornerRadius = UDim.new(1, 0),
                Parent = ToggleDot
            })
            
            local function UpdateToggle()
                if value then
                    Tween(ToggleDot, {Position = UDim2.new(1, -14, 0, 2)}, 0.2)
                    Tween(ToggleButton, {BackgroundColor3 = Library.Theme.ToggleColor:lerp(Color3.new(1,1,1), 0.3)}, 0.2)
                else
                    Tween(ToggleDot, {Position = UDim2.new(0, 2, 0, 2)}, 0.2)
                    Tween(ToggleButton, {BackgroundColor3 = Color3.fromRGB(50, 50, 50)}, 0.2)
                end
            end
            
            UpdateToggle()
            
            ToggleButton.InputBegan:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    value = not value
                    UpdateToggle()
                    if callback then callback(value) end
                end
            end)
            
            Toggle.GetValue = function() return value end
            Toggle.SetValue = function(newValue)
                value = newValue
                UpdateToggle()
                if callback then callback(value) end
            end
            
            table.insert(MultiSection.Elements, Toggle)
            return Toggle
        end
        
        function MultiSection:AddTextbox(name, placeholder, callback)
            local Textbox = {}
            local value = ""
            
            local TextboxFrame = Create("Frame", {
                Name = "Textbox_"..name,
                BackgroundTransparency = 1,
